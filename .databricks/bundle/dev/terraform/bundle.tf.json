{
  "terraform": {
    "required_providers": {
      "databricks": {
        "source": "databricks/databricks",
        "version": "1.50.0"
      }
    }
  },
  "provider": {
    "databricks": {}
  },
  "resource": {
    "databricks_job": {
      "dbt": {
        "deployment": {
          "kind": "BUNDLE",
          "metadata_file_path": "/dbt_dev/my_project/dev/state/metadata.json"
        },
        "edit_mode": "UI_LOCKED",
        "email_notifications": {
          "on_failure": [
            "palash.patidar@koantek.com"
          ]
        },
        "format": "MULTI_TASK",
        "name": "dbt",
        "queue": {
          "enabled": true
        },
        "tags": {},
        "task": [
          {
            "disable_auto_optimization": true,
            "min_retry_interval_millis": 900000,
            "notebook_task": {
              "notebook_path": "/dbt_dev/my_project/dev/files/src/dummy",
              "source": "WORKSPACE"
            },
            "task_key": "dummy_run"
          }
        ]
      }
    }
  }
}
